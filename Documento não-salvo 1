// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int paulo;
    int jose;
    int soma;
    int multiplicação;
    paulo = 30;
    jose = 24;
    soma = paulo + jose;
    multiplicação = paulo * jose;
    printf("o valor de paulo é %d \n", paulo);
    printf("o valor de jose é %d \n", jose);
    printf(" a soma e %d \n", soma);
printf(" a multiplicação é %d", multiplicação);
    return 0;
}
